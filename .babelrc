{
  "presets": ["react", "es2015-node6/object-rest"],
  "plugins": [
    "add-module-exports",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }],
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "bluebird-async-functions"
  ],
  "ignore": [
    "node_modules",
    "server"
  ]
}
